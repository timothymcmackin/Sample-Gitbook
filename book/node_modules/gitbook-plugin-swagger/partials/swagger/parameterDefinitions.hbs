{{!--
    Global parameter definitions (see https://github.com/swagger-api/swagger-spec/blob/master/versions/2.0.md#parametersDefinitionsObject)
    @param {object<Parameter>} parameters the parameter-definitions object
    @api public
--}}
{{#if parameters}}
    <h2>Parameter definitions</h2>
    <table id="table-parameter-definitions" class="table">
        <thead>
        <tr>
            <th class="sw-param-key">Key</th>
            <th class="sw-param-name">Name</th>
            <th class="sw-param-description">Description</th>
            <th class="sw-param-type">Type</th>
            <th class="sw-param-data-type">Data Type</th>
            <th class="sw-param-annotation">Annotation</th>
        </tr>
        </thead>
        <tbody>
        {{#each parameters}}
            {{> swagger/parameterRow key=@key parameter=.}}
        {{/each}}
        </tbody>
    </table>
{{/if}}
 